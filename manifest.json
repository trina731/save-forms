{
    "name": "bear",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.5.1.min.js", "content.js"]
      }
    ],
    "browser_action": {
      "default_title": "View Submitted Forms"
    },
    "background": {
      "scripts": ["jquery-3.5.1.min.js", "background.js"]
    },
    "permissions": ["tabs"]
  }